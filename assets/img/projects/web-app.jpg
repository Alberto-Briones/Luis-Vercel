<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="webGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="300" fill="#f8f9fa"/>
  
  <!-- Browser window -->
  <rect x="20" y="20" width="360" height="260" rx="8" fill="white" stroke="#e9ecef" stroke-width="2"/>
  
  <!-- Browser header -->
  <rect x="20" y="20" width="360" height="40" rx="8" fill="#f1f3f4"/>
  <rect x="20" y="20" width="360" height="8" rx="8" fill="#f1f3f4"/>
  
  <!-- Browser controls -->
  <circle cx="40" cy="40" r="6" fill="#ff5f57"/>
  <circle cx="60" cy="40" r="6" fill="#ffbd2e"/>
  <circle cx="80" cy="40" r="6" fill="#28ca42"/>
  
  <!-- Address bar -->
  <rect x="100" y="32" width="200" height="16" rx="8" fill="white" stroke="#e9ecef"/>
  <text x="110" y="42" font-family="Arial" font-size="10" fill="#666">https://mi-aplicacion.vercel.app</text>
  
  <!-- Content area -->
  <rect x="30" y="70" width="340" height="200" fill="url(#webGradient)" opacity="0.1"/>
  
  <!-- Web elements -->
  <rect x="50" y="90" width="80" height="20" rx="4" fill="#667eea"/>
  <text x="70" y="102" font-family="Arial" font-size="10" fill="white" text-anchor="middle">Header</text>
  
  <rect x="50" y="120" width="300" height="60" rx="4" fill="white" stroke="#e9ecef"/>
  <text x="200" y="140" font-family="Arial" font-size="12" fill="#333" text-anchor="middle">Contenido Principal</text>
  <text x="200" y="155" font-family="Arial" font-size="10" fill="#666" text-anchor="middle">HTML, CSS, JavaScript</text>
  
  <rect x="50" y="190" width="100" height="30" rx="4" fill="#28ca42"/>
  <text x="100" y="207" font-family="Arial" font-size="10" fill="white" text-anchor="middle">Botón</text>
  
  <rect x="170" y="190" width="100" height="30" rx="4" fill="#667eea"/>
  <text x="220" y="207" font-family="Arial" font-size="10" fill="white" text-anchor="middle">Formulario</text>
  
  <!-- Analytics icon -->
  <circle cx="320" cy="240" r="15" fill="#4285f4"/>
  <text x="320" y="245" font-family="Arial" font-size="12" fill="white" text-anchor="middle">GA4</text>
  
  <!-- Vercel logo -->
  <text x="50" y="250" font-family="Arial" font-size="12" fill="#000" font-weight="bold">▲ Vercel</text>
</svg>

















