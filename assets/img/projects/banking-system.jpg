<svg width="400" height="250" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg3" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#dc3545;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#fd7e14;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="100%" height="100%" fill="url(#bg3)"/>
  <g transform="translate(50,50)">
    <!-- Database Icon -->
    <ellipse cx="100" cy="40" rx="40" ry="15" fill="rgba(255,255,255,0.3)"/>
    <rect x="60" y="40" width="80" height="60" fill="rgba(255,255,255,0.2)"/>
    <ellipse cx="100" cy="100" rx="40" ry="15" fill="rgba(255,255,255,0.3)"/>
    
    <!-- Server/Application -->
    <rect x="200" y="30" width="60" height="80" fill="rgba(255,255,255,0.25)" rx="5"/>
    <rect x="210" y="40" width="40" height="5" fill="rgba(255,255,255,0.4)"/>
    <rect x="210" y="50" width="40" height="5" fill="rgba(255,255,255,0.4)"/>
    <rect x="210" y="60" width="40" height="5" fill="rgba(255,255,255,0.4)"/>
    
    <!-- Connection Lines -->
    <line x1="140" y1="50" x2="190" y2="50" stroke="rgba(255,255,255,0.6)" stroke-width="3"/>
    <line x1="140" y1="70" x2="190" y2="70" stroke="rgba(255,255,255,0.6)" stroke-width="3"/>
    
    <!-- Code Elements -->
    <text x="50" y="140" fill="rgba(255,255,255,0.4)" font-family="monospace" font-size="12">Java</text>
    <text x="50" y="155" fill="rgba(255,255,255,0.4)" font-family="monospace" font-size="12">Oracle SQL</text>
    <text x="50" y="170" fill="rgba(255,255,255,0.4)" font-family="monospace" font-size="12">Struts</text>
    
    <text x="200" y="180" text-anchor="middle" fill="white" font-family="Arial" font-size="24" font-weight="bold">Banking System</text>
    <text x="200" y="200" text-anchor="middle" fill="rgba(255,255,255,0.8)" font-family="Arial" font-size="14">Inbursa Manufacturing</text>
  </g>
